FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 600;
    }
    nozzle
    {
        type            zeroGradient;
    }
    outlet_left
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    outlet_right
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    outlet_topBottom
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
}


