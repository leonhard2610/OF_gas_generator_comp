FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    nozzle
    {
        type            omegaWallFunction;
        value           uniform 1;
    }
    outlet_left
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    outlet_right
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    outlet_topBottom
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to5
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to7
    {
        type            processor;
        value           uniform 1;
    }
}


