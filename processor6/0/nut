FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    nozzle
    {
        type            nutkWallFunction;
        value           nonuniform List<scalar> 0();
    }
    outlet_left
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    outlet_right
    {
        type            calculated;
        value           uniform 0;
    }
    outlet_topBottom
    {
        type            calculated;
        value           uniform 0;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 0;
    }
}


